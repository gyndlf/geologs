{
    "display_information": {
        "name": "rocky",
        "description": "The latest rock updates",
        "background_color": "#004492"
    },
    "features": {
        "bot_user": {
            "display_name": "rocky",
            "always_online": true
        }
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "app_mentions:read",
                "channels:history",
                "channels:join",
                "channels:manage",
                "channels:read",
                "channels:write.topic",
                "chat:write",
                "chat:write.public",
                "incoming-webhook",
                "reactions:read",
                "reactions:write"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "app_mention"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}